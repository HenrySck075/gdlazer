{
  "geode": "4.5.0",
  "gd": {
    "win": "2.2074",
    "android": "2.2074",
    "mac": "2.2074",
    "ios": "2.2074"
  },
  "id": "henrysck075.gdlazer",
  "name": "gd!lazer",
  "version": "v0.0.7",
  "developer": "Henry Spheria",
  "description": "jumping is just a *click* away!",
  "early-load": true,
  "settings": {
    "language": {
      "type": "string",
      "default": "English",
      "one-of": [
        "English",
        "Vietnamese"
      ],
      "description": "Changes the UI language. Currently does nothing as it hasn't referenced the localization files yet."
    },
    "parallax": {
      "type": "bool", 
      "default": true,
      "description": "Enables parallax effects"
    },
    "devmode": {
      "name": "this menu does not work dont enable",
      "type": "bool",
      "default": true
    },
    "dont": {
      "name": "Just, don't.",
      "description": "Does not do anything... Well, nothing useful.",
      "type": "bool",
      "default": false
    },
    "uiscale": {
      "type": "float",
      "name": "UI Scale",
      "default": 1,
      "min": 0.1,
      "max": 2,
      "control": {
        "slider": true
      },
      "description": "self-explanatory except it doesnt work because its not implemented"
    }
  },
  "resources": {
    "spritesheets": {
      "MainMenuSheet": [
        "resources/textures/menu/*.png"
      ],
      "UISheet": [
        "resources/textures/ui/*.png"
      ],
      "IntroSheet": [
        "resources/textures/iconset/*.png",
        "resources/textures/triangle/*.png"
      ]
    },
    "files": [
      "resources/*.mp3",
      "resources/sfx/gameplay/*.wav",
      "resources/sfx/menu/*.wav",
      "resources/sfx/ui/*.wav",
      "resources/sfx/gameplay/pause-loop.mp3",
      "resources/glyphs/*.fnt",
      "resources/glyphs/*.png",
      "resources/fonts/**/*.fnt",
      "resources/fonts/**/*.png",
      "resources/fonts/*.png",
      "resources/torus/*.ttf",
      "resources/textures/cursor/*.png"
    ]
  },
  "dependencies": {
    "henrysck075.imagecache": {
      "importance": "required",
      "version": "v1.0.4"
    },
    "alphalaneous.happy_textures": {
      "importance": "recommended",
      "version": ">=v1.6.1"
    }
  },
  "tags": ["enhancement","interface","cheats"]
}
