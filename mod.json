{
  "geode": "3.8.1",
  "gd": {
    "win": "2.206",
    "android": "2.206",
    "mac": "2.206",
    "ios": "2.206"
  },
  "id": "henrysck075.osulazer",
  "name": "gd!lazer",
  "version": "v0.0.7",
  "developer": "Henry Spheria",
  "description": "nico nico niii",
  "early-load": true,
  "settings": {
    "language": {
      "type": "string",
      "default": "English",
      "one-of": [
        "English",
        "Vietnamese"
      ]
    },
    "parallax": {
      "type": "bool",
      "default": true,
      "description": "Enables parallax effects"
    },
    "uiscale": {
      "type": "int",
      "name": "UI Scale",
      "default": 1,
      "min": 1,
      "max": 20,
      "control": {
        "slider": true
      },
      "description": "Contradict to the way game engines implements scaling, this one goes the opposite way"
    }
  },
  "resources": {
    "spritesheets": {
      "MainMenuSheet": [
        "resources/textures/menu/*.png"
      ],
      "UISheet": [
        "resources/textures/ui/*.png"
      ],
      "IntroSheet": [
        "resources/textures/iconset/*.png",
        "resources/textures/triangle/*.png"
      ]
    },
    "files": [
      "resources/triangles.mp3",
      "resources/sfx/**/*.wav",
      "resources/sfx/gameplay/pause-loop.mp3",
      "resources/glyphs/*.fnt",
      "resources/glyphs/*.png",
      "resources/torus/*.ttf"
    ]
  },
  "dependencies": [
    {
      "id": "henrysck075.imagecache",
      "importance": "required",
      "version": "v1.0.0"
    }
  ]
}
