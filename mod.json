{
	"geode": "3.3.0",
	"gd": {
    "win": "2.206",
    "android": "2.206",
    "mac": "2.206",
    "ios": "2.206"
	},
	"id": "henrysck075.osulazer",
	"name": "osu!lazer",
	"version": "v1.0.0",
	"developer": "Nico Yazawa",
	"description": "nico nico niii",
	"early-load": true,
  "dependencies": [
    {
      "id": "henrysck075.cocosutils",
      "version": ">=v0.0.7",
      "importance": "required"
    },
    {
      "id": "spaghettdev.betterinputs",
      "version": ">=4.2.0-beta.1",
      "importance": "recommended"
    }
  ],
	"resources": {
    "fonts": {
    	"torus-regular": {
        "path": "resources/torus/regular.ttf",
        "size": 80
    	},
    	"torus-light": {
        "path": "resources/torus/light.ttf",
        "size": 80
    	},
    	"torus-bold": {
        "path": "resources/torus/bold.ttf",
        "size": 80
    	}
    },
    "sprites": [
      "resources/textures/ui/dialog.png"
    ],
    "spritesheets": {
      "MainMenuSheet": [
        "resources/textures/menu/*.png",
        "resources/textures/menu/*.jpg"
      ],
      "UISheet": [
        "resources/textures/ui/square.png"
      ],
      "IntroSheet": [
        "resources/textures/iconset/*.png",
        "resources/textures/triangle/*.png"
      ]
    },
    "files": [
      "resources/triangles.mp3",
      "resources/sfx/**/*.wav",
      "resources/sfx/gameplay/pause-loop.mp3",
      "resources/glyphs/*.fnt",
      "resources/glyphs/*.png"
    ]
	}
}
